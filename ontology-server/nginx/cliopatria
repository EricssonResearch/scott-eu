    listen 80;
    listen [::]:80;

    server_name %ONTOLOGY_HOSTNAME%;

    location / {
      #rewrite /(.*)      /api/export_graph?graph=http%3A%2F%2F%ONTOLOGY_HOSTNAME%%2F$1&mimetype=text%2Fplain&format=rdfxml break;
      proxy_pass         http://cliopatria:3020;
