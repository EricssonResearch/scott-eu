@prefix	lib: <http://www.swi-prolog.org/rdf/library/> .
@prefix	dcterms: <http://purl.org/dc/terms/> .
@prefix	oslc: <http://open-services.net/ns/core#> .
@prefix	oslc_shapes: <http://ontology.cf.ericsson.net/oslc_shapes/> .
@prefix	oslc_example: <http://ontology.cf.ericsson.net/oslc_example/> .

<oslc>
	a lib:Schema ;
	a lib:Virtual ;
	dcterms:title "All vocabulary URIs defined in the OSLC Core namespace" ;
	lib:source oslc: ;
	lib:schema <oslc.rdf> .

[ a lib:Namespace ;
	lib:mnemonic "oslc" ;
	lib:namespace oslc:
] .

<oslc_shapes>
	a lib:Schema ;
	a lib:Virtual ;
	dcterms:title "OSLC resource shapes" ;
	lib:source <http://ontology.cf.ericsson.net/oslc_shapes#> ;
	lib:schema <oslc_shapes.ttl> .

[ a lib:Namespace ;
	lib:mnemonic "oslc_shapes" ;
	lib:namespace oslc_shapes:
] .

<oslc_example>
	a lib:Schema ;
	a lib:Virtual ;
	dcterms:title "An example OSLC Service Provider Catalog" ;
	lib:source <http://ontology.cf.ericsson.net/oslc_example#> ;
	lib:schema <oslc_example.ttl> .

[ a lib:Namespace ;
	lib:mnemonic "oslc_example" ;
	lib:namespace oslc_example:
] .
