@prefix	lib: <http://www.swi-prolog.org/rdf/library/> .
@prefix	dcterms: <http://purl.org/dc/terms/> .
@prefix	oslc: <http://open-services.net/ns/core#> .
@prefix	oslc_shapes: <http://ontology.cf.ericsson.net/oslc_shapes/> .
@prefix	oslc_test: <http://ontology.cf.ericsson.net/oslc_test/> .

<oslc>
	a lib:Schema ;
	a lib:Virtual ;
	dcterms:title "All vocabulary URIs defined in the OSLC Core namespace" ;
	lib:source oslc: ;
	lib:Namespace [ a lib:Namespace ;
                  lib:mnemonic "oslc" ;
                  lib:namespace oslc:
	              ] ;
	lib:schema <oslc.rdf> .

<oslc_shapes>
	a lib:Schema ;
	a lib:Virtual ;
	dcterms:title "OSLC resource shapes" ;
	lib:source oslc_shapes: ;
	lib:Namespace [ a lib:Namespace ;
                  lib:mnemonic "oslc_shapes" ;
                  lib:namespace oslc_shapes:
	              ] ;
	lib:schema <oslc_shapes.ttl> .

<oslc_test>
	a lib:Schema ;
	a lib:Virtual ;
	dcterms:title "OSLC Service Provider Test" ;
	lib:source oslc_test: ;
	lib:Namespace [ a lib:Namespace ;
                  lib:mnemonic "oslc_test" ;
                  lib:namespace oslc_test:
	              ] ;
	lib:schema <oslc_test.ttl> .
