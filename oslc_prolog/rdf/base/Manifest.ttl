@prefix	lib: <http://www.swi-prolog.org/rdf/library/> .
@prefix	dcterms: <http://purl.org/dc/terms/> .
@prefix	oslc: <http://open-services.net/ns/core#> .
@prefix	oslcs: <http://ontology.cf.ericsson.net/oslc_shapes#> .
@prefix	oslcp: <http://ontology.cf.ericsson.net/oslc_prolog#> .

<oslc>
	a lib:Schema ;
	a lib:Virtual ;
	dcterms:title "All vocabulary URIs defined in the OSLC Core namespace" ;
	lib:source oslc: ;
	lib:schema <oslc.rdf> .

<oslcs>
	a lib:Schema ;
	a lib:Virtual ;
	dcterms:title "OSLC resource shapes" ;
	lib:source oslcs: ;
	lib:schema <oslc_shapes.ttl> .

<oslcp>
	a lib:Schema ;
	a lib:Virtual ;
	dcterms:title "Prolog names of OSLC resources" ;
	lib:source oslcp: ;
	lib:schema <oslc_prolog.ttl> .
