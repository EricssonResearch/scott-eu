@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix	oslc: <http://open-services.net/ns/core#> .
@prefix	oslcs: <http://ontology.cf.ericsson.net/oslc_shapes#> .
@prefix	: <http://ontology.cf.ericsson.net/oslc_prolog#> .
@base	<http://ontology.cf.ericsson.net/oslc_prolog> .


:PrologResource
  a rdfs:Class .

:prologModule
  a rdf:Property .

:prologPredicate
  a rdf:Property .

# ======== Service Provider Catalog

:serviceProviderCatalog
  a :PrologResource ;
  :prologModule "oslc"^^xsd:string ;
  :prologPredicate "service_provider_catalog"^^xsd:string ;
  oslc:resourceShape oslcs:serviceProviderCatalogShape .

