@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix	dcterms: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix	oslc: <http://open-services.net/ns/core#> .
@prefix	shapes: <http://ontology.cf.ericsson.net/oslc_shapes/> .
@prefix	: <http://ontology.cf.ericsson.net/oslc_example/> .


:leoSPC
  a oslc:ServiceProviderCatalog ;
  oslc:instanceShape shapes:serviceProviderCatalogShape ;
  dcterms:title "Leo's Service Provider Catalog"^^rdf:XMLLiteral ;
  dcterms:publisher [ a oslc:Publisher ;
                      dcterms:title "Leo"^^rdf:XMLLiteral ;
                      dcterms:identifier "1122-33"^^xsd:string ;
                    ] ;
  oslc:serviceProvider :leoSP .

:leoSP
  a oslc:ServiceProvider ;
  oslc:instanceShape shapes:serviceProviderShape ;
  dcterms:title "Leo's Service Provider"^^rdf:XMLLiteral ;
  oslc:service [ a oslc:Service ;
                 oslc:domain <http://leo.com/domain> ;
                 oslc:creationFactory :leoCF
               ] .

:leoCF
  a oslc:CreationFactory ;
  oslc:instanceShape shapes:creationFactoryShape ;
  dcterms:title "Leo's Creation Factory"^^rdf:XMLLiteral ;
  oslc:resourceShape :leoResourceShape .

:leoQC
  a oslc:QueryCapability ;
  oslc:instanceShape shapes:queryCapabilityShape ;
  dcterms:title "Leo's Query Capability"^^rdf:XMLLiteral ;
  oslc:resourceShape :leoResourceShape .


:leoResourceShape
  a oslc:ResourceShape ;
  oslc:instanceShape shapes:resourceShapeShape ;
  dcterms:title "Leo's Resource Shape"^^rdf:XMLLiteral ;
  oslc:describes :leoResourceClass ;
  oslc:property [ a oslc:Property ;
                  oslc:name "title"^^xsd:string ;
                  oslc:propertyDefinition dcterms:title ;
                  oslc:occurs oslc:Exactly-one ;
                  oslc:readOnly true ;
                  oslc:valueType rdf:XMLLiteral
                ] ,
                [ a oslc:Property ;
                  oslc:name "internal"^^xsd:string ;
                  oslc:propertyDefinition dcterms:publisher ;
                  oslc:occurs oslc:Zero-or-one ;
                  oslc:readOnly true ;
                  oslc:valueType oslc:LocalResource ;
                  oslc:representaton oslc:Inline
                ] .

:leoResourceClass
  a rdfs:Class ;
  dcterms:title "Leo's Resource Class"^^rdf:XMLLiteral .
