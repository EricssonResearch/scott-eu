@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://ontology.cf.ericsson.net/pddl/> .

:Domain
  a rdfs:Class ;
  rdfs:subClassOf rdf:Bag .

:AbstractClass
  a rdfs:Class .

:Type
  a rdfs:Class ;
  rdfs:subClassOf :AbstractClass .
:PrimitiveType
  a rdfs:Class ;
  rdfs:subClassOf :Type .
:EitherType
  a rdfs:Class ;
  rdfs:subClassOf :Type ;
  rdfs:subClassOf rdf:Bag .

:Action
  a rdfs:Class ;
  rdfs:subClassOf :Parameterizable .
:parameter
  a rdf:Property .
:precondition
  a rdf:Property .
:Precondition
  a rdfs:Class ;
  rdfs:subClassOf :AbstractClass .
:effect
  a rdf:Property .
:Effect
  a rdfs:Class ;
  rdfs:subClassOf :AbstractClass .
:PredicateEffect
  a rdfs:Class ;
  rdfs:subClassOf :AbstractClass ;
  rdfs:subClassOf :Effect .

:Parameter
  a rdfs:Class ;
  rdfs:subClassOf rdf:Property ;
  rdfs:subClassOf :Orderable .
:type
  a rdf:Property .

:Orderable
  a rdfs:Class ;
  rdfs:subClassOf :AbstractClass .

:Parameterizable
  a rdfs:Class ;
  rdfs:subClassOf :AbstractClass .

:Predicate
  a rdfs:Class ;
  rdfs:subClassOf :Parameterizable ;
  rdfs:subClassOf :Precondition ;
  rdfs:subClassOf :PredicateEffect .

:Fluent
  a rdfs:Class ;
  rdfs:subClassOf :AbstractClass ;
  rdfs:subClassOf :Precondition .

:Function
  a rdfs:Class ;
  rdfs:subClassOf :Parameterizable ;
  rdfs:subClassOf :Fluent .

:Number
  a rdfs:Class ;
  rdfs:subClassOf :Fluent .

:argument
  a rdf:Property .

:Object
  a rdfs:Class .

# --- Logical Operators

:LogicalOperator
  a rdfs:Class ;
  rdfs:subClassOf :AbstractClass ;
  rdfs:subClassOf :Precondition .

:And
  a rdfs:Class ;
  rdfs:subClassOf :LogicalOperator ;
  rdfs:subClassOf :Effect ;
  rdfs:label "and" .

:Not
  a rdfs:Class ;
  rdfs:subClassOf :LogicalOperator ;
  rdfs:subClassOf :PredicateEffect ;
  rdfs:label "not" .

:Or
  a rdfs:Class ;
  rdfs:subClassOf :LogicalOperator ;
  rdfs:label "or" .

:Imply
  a rdfs:Class ;
  rdfs:subClassOf :LogicalOperator ;
  rdfs:label "imply" .

# --- Binary Operators

:BinaryOperator
  a rdfs:Class ;
  rdfs:subClassOf :AbstractClass ;
  rdfs:subClassOf :Fluent .

:Plus
  a rdfs:Class ;
  rdfs:subClassOf :BinaryOperator ;
  rdfs:label "+" .

:Minus
  a rdfs:Class ;
  rdfs:subClassOf :BinaryOperator ;
  rdfs:label "-" .

:Multiply
  a rdfs:Class ;
  rdfs:subClassOf :BinaryOperator ;
  rdfs:label "*" .

:Divide
  a rdfs:Class ;
  rdfs:subClassOf :BinaryOperator ;
  rdfs:label "/" .

# --- Binary Comparators

:BinaryComparator
  a rdfs:Class ;
  rdfs:subClassOf :AbstractClass ;
  rdfs:subClassOf :Precondition .

:EQ
  a rdfs:Class ;
  rdfs:subClassOf :BinaryComparator ;
  rdfs:label "=" .

:GT
  a rdfs:Class ;
  rdfs:subClassOf :BinaryComparator ;
  rdfs:label ">" .

:LT
  a rdfs:Class ;
  rdfs:subClassOf :BinaryComparator ;
  rdfs:label "<" .

:LE
  a rdfs:Class ;
  rdfs:subClassOf :BinaryComparator ;
  rdfs:label "<=" .

:GE
  a rdfs:Class ;
  rdfs:subClassOf :BinaryComparator ;
  rdfs:label ">=" .

# --- Quantifiers

:Quantifier
  a rdfs:Class ;
  rdfs:subClassOf :AbstractClass ;
  rdfs:subClassOf :Precondition .

:ForAll
  a rdfs:Class ;
  rdfs:subClassOf :Quantifier ;
  rdfs:subClassOf :Parameterizable ;
  rdfs:subClassOf :Effect ;
  rdfs:label "forall" .

:Exists
  a rdfs:Class ;
  rdfs:subClassOf :Quantifier ;
  rdfs:subClassOf :Parameterizable ;
  rdfs:label "exists" .

# --- Conditional Effect

:When
  a rdfs:Class ;
  rdfs:subClassOf :Effect ;
  rdfs:label "when" .

# --- Assignment Operators

:AssignmentOperator
  a rdfs:Class ;
  rdfs:subClassOf :AbstractClass ;
  rdfs:subClassOf :PredicateEffect .

:Assign
  a rdfs:Class ;
  rdfs:subClassOf :AssignmentOperator ;
  rdfs:label "assign" .

:ScaleUp
  a rdfs:Class ;
  rdfs:subClassOf :AssignmentOperator ;
  rdfs:label "scale-up" .

:ScaleDown
  a rdfs:Class ;
  rdfs:subClassOf :AssignmentOperator ;
  rdfs:label "scale-down" .

:Increase
  a rdfs:Class ;
  rdfs:subClassOf :AssignmentOperator ;
  rdfs:label "increase" .

:Decrease
  a rdfs:Class ;
  rdfs:subClassOf :AssignmentOperator ;
  rdfs:label "decrease" .
