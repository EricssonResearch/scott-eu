@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix oslc: <http://open-services.net/ns/core#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix : <http://ontology.cf.ericsson.net/pddl/> .

# --- Abstract Classes
# These classes are never instantiated

:MetaClass
  a rdfs:Class .

:Type
  a rdfs:Class .

:Orderable
  a rdfs:Class .

:Parameterizable
  a rdfs:Class .

:LogicalOperator
  a rdfs:Class .

:BinaryOperator
  a rdfs:Class .

:BinaryComparator
  a rdfs:Class .

:Quantifier
  a rdfs:Class .

:AssignmentOperator
  a rdfs:Class .

# --- PDDL Meta Classes
# Users create subclasses of these classes

:PrimitiveType
  a rdfs:Class ;
  rdfs:subClassOf :Type ;
  rdfs:subClassOf :MetaClass .

:EitherType
  a rdfs:Class ;
  rdfs:subClassOf :Type ;
  rdfs:subClassOf rdf:Bag ;
  rdfs:subClassOf :MetaClass .

:Action
  a rdfs:Class ;
  rdfs:subClassOf :MetaClass ;
  rdfs:subClassOf :Parameterizable .

:Predicate
  a rdfs:Class ;
  rdfs:subClassOf :MetaClass ;
  rdfs:subClassOf :Parameterizable .

:Function
  a rdfs:Class ;
  rdfs:subClassOf :MetaClass ;
  rdfs:subClassOf :Parameterizable .

# --- PDDL Classes
# Users create instances of these classes

:Domain
  a rdfs:Class .

:Problem
  a rdfs:Class .

:Parameter
  a rdfs:Class ;
  rdfs:subClassOf rdf:Property ;
  rdfs:subClassOf :Orderable .

:Equality # requirement :equality
  a rdfs:Class ;
  rdfs:subClassOf :Predicate ;
  oslc:instanceShape :PredicateShape ;
  rdfs:label "=" ;
  :parameter :left ,
             :right .

:Plan
  a rdfs:Class .

:Step
  a rdfs:Class .

# --- PDDL Instances

:total-time
  a rdfs:Class ;
  rdfs:subClassOf :Function ;
  oslc:instanceShape :FunctionShape ;
  rdfs:label "total-time" .

:argument
  a :Parameter ;
  oslc:instanceShape :ParameterShape ;
  sh:order 1 .

:left
  a :Parameter ;
  oslc:instanceShape :ParameterShape ;
  sh:order 1 .

:right
  a :Parameter ;
  oslc:instanceShape :ParameterShape ;
  sh:order 2 .

# --- PDDL Properties

:type
  a rdf:Property .

:constant
  a rdf:Property .

:predicate
  a rdf:Property .

:function
  a rdf:Property .

:action
  a rdf:Property .

:domain
  a rdf:Property .

:object
  a rdf:Property .

:init
  a rdf:Property .

:goal
  a rdf:Property .

:parameter
  a rdf:Property .

:precondition
  a rdf:Property .

:effect
  a rdf:Property .

:minimize
  a rdf:Property .

:maximize
  a rdf:Property .

:step
  a rdf:Property .

:cost
  a rdf:Property .

:time
  a rdf:Property .

# --- Logical Operators

:And
  a rdfs:Class ;
  rdfs:subClassOf :LogicalOperator ;
  rdfs:label "and" .

:Not
  a rdfs:Class ;
  rdfs:subClassOf :LogicalOperator ;
  rdfs:label "not" .

:Or
  a rdfs:Class ;
  rdfs:subClassOf :LogicalOperator ;
  rdfs:label "or" .

:Imply
  a rdfs:Class ;
  rdfs:subClassOf :LogicalOperator ;
  rdfs:label "imply" .

# --- Binary Operators

:Plus
  a rdfs:Class ;
  rdfs:subClassOf :BinaryOperator ;
  rdfs:label "+" .

:Minus
  a rdfs:Class ;
  rdfs:subClassOf :BinaryOperator ;
  rdfs:label "-" .

:Multiply
  a rdfs:Class ;
  rdfs:subClassOf :BinaryOperator ;
  rdfs:label "*" .

:Divide
  a rdfs:Class ;
  rdfs:subClassOf :BinaryOperator ;
  rdfs:label "/" .

# --- Binary Comparators

:EQ
  a rdfs:Class ;
  rdfs:subClassOf :BinaryComparator ;
  rdfs:label "=" .

:GT
  a rdfs:Class ;
  rdfs:subClassOf :BinaryComparator ;
  rdfs:label ">" .

:LT
  a rdfs:Class ;
  rdfs:subClassOf :BinaryComparator ;
  rdfs:label "<" .

:LE
  a rdfs:Class ;
  rdfs:subClassOf :BinaryComparator ;
  rdfs:label "<=" .

:GE
  a rdfs:Class ;
  rdfs:subClassOf :BinaryComparator ;
  rdfs:label ">=" .

# --- Quantifiers

:ForAll
  a rdfs:Class ;
  rdfs:subClassOf :Quantifier ;
  rdfs:subClassOf :Parameterizable ;
  rdfs:label "forall" .

:Exists
  a rdfs:Class ;
  rdfs:subClassOf :Quantifier ;
  rdfs:subClassOf :Parameterizable ;
  rdfs:label "exists" .

# --- Conditional Effect

:When
  a rdfs:Class ;
  rdfs:label "when" .

# --- Assignment Operators

:Assign
  a rdfs:Class ;
  rdfs:subClassOf :AssignmentOperator ;
  rdfs:label "assign" .

:ScaleUp
  a rdfs:Class ;
  rdfs:subClassOf :AssignmentOperator ;
  rdfs:label "scale-up" .

:ScaleDown
  a rdfs:Class ;
  rdfs:subClassOf :AssignmentOperator ;
  rdfs:label "scale-down" .

:Increase
  a rdfs:Class ;
  rdfs:subClassOf :AssignmentOperator ;
  rdfs:label "increase" .

:Decrease
  a rdfs:Class ;
  rdfs:subClassOf :AssignmentOperator ;
  rdfs:label "decrease" .

# --- Resource Shapes

:ActionShape
  a oslc:ResourceShape ;
  dcterms:description "An action has parameter, precondition and effect."^^rdf:XMLLiteral ;
  oslc:property [ a oslc:Property ;
                  dcterms:description "Action name."^^rdf:XMLLiteral ;
                  oslc:propertyDefinition rdfs:label ;
                  oslc:name "label"^^xsd:string ;
                  oslc:occurs oslc:Exactly-one ;
                  oslc:valueType xsd:string
                ] ,
                [ a oslc:Property ;
                  dcterms:description "Action parameters."^^rdf:XMLLiteral ;
                  oslc:propertyDefinition :parameter ;
                  oslc:name "parameter"^^xsd:string ;
                  oslc:occurs oslc:Zero-or-many ;
                  oslc:valueType oslc:AnyResource ;
                  oslc:range :Parameter
                ] ,
                [ a oslc:Property ;
                  dcterms:description "Action precondition that must be fulfilled to enable it."^^rdf:XMLLiteral ;
                  oslc:propertyDefinition :precondition ;
                  oslc:name "precondition"^^xsd:string ;
                  oslc:occurs oslc:Exactly-one ;
                  oslc:valueType oslc:AnyResource
                ] ,
                [ a oslc:Property ;
                  dcterms:description "Action effect describing state modification."^^rdf:XMLLiteral ;
                  oslc:propertyDefinition :effect ;
                  oslc:name "effect"^^xsd:string ;
                  oslc:occurs oslc:Exactly-one ;
                  oslc:valueType oslc:AnyResource
                ] .

:PredicateShape
  a oslc:ResourceShape ;
  dcterms:description "A predicate."^^rdf:XMLLiteral ;
  oslc:property [ a oslc:Property ;
                  dcterms:description "Predicate name."^^rdf:XMLLiteral ;
                  oslc:propertyDefinition rdfs:label ;
                  oslc:name "label"^^xsd:string ;
                  oslc:occurs oslc:Exactly-one ;
                  oslc:valueType xsd:string
                ] ,
                [ a oslc:Property ;
                  dcterms:description "Predicate parameters."^^rdf:XMLLiteral ;
                  oslc:propertyDefinition :parameter ;
                  oslc:name "parameter"^^xsd:string ;
                  oslc:occurs oslc:Zero-or-many ;
                  oslc:valueType oslc:AnyResource ;
                  oslc:range :Parameter
                ] .

:FunctionShape
  a oslc:ResourceShape ;
  dcterms:description "Function."^^rdf:XMLLiteral ;
  oslc:property [ a oslc:Property ;
