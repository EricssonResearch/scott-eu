:adl-blocksworld-problem
  a pddl:Problem ;
  oslc:instanceShape pddls:ProblemShape ;
  rdfs:label "adl-blocksworld-problem" ;
  pddl:domain pddld:adl-blocksworld ;
  pddl:object :a ,
              :b ,
              :c ,
              pddld:table ;
  pddl:init [ a pddld:on ;
              pddld:on-x :b ;
              pddld:on-y :table
            ] ,
            [ a pddld:on ;
              pddld:on-x :a ;
              pddld:on-y :table
            ] ,
            [ a pddld:on ;
              pddld:on-x :c ;
              pddld:on-y :a
            ] ,
            [ a pddld:clear ;
              pddld:clear-x :b
            ] ,
            [ a pddld:clear ;
              pddld:clear-x :c
            ] ,
            [ a pddld:clear ;
              pddld:clear-x :table
            ] ;
  pddl:goal [ a pddl:Or ;
              pddl:argument [ a pddld:on ;
                              pddld:on-x :b ;
                              pddld:on-y :c
                            ] ,
                            [ a pddld:on ;
                              pddld:on-x :c ;
                              pddld:on-y :b
                            ]
            ] ;
  pddl:minimize pddl:total-time .

:a
  a pddl:Object ;
  oslc:instanceShape pddls:ObjectShape ;
  rdfs:label "a" .

:b
  a pddl:Object ;
  oslc:instanceShape pddls:ObjectShape ;
  rdfs:label "b" .

:c
  a pddl:Object ;
  oslc:instanceShape pddls:ObjectShape ;
  rdfs:label "c" .
