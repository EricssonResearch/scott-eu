      <resources name="DirectSelector" extends="//@specification/@domainSpecifications[name='MissionOntology']/@resources[name='Selector']" resourceProperties="//@specification/@domainSpecifications[name='MissionOntology']/@resourceProperties[name='item']"/>
      <resources name="TypeSelector" extends="//@specification/@domainSpecifications[name='MissionOntology']/@resources[name='Selector']" resourceProperties="//@specification/@domainSpecifications[name='MissionOntology']/@resourceProperties[name='itemType'] //@specification/@domainSpecifications[name='MissionOntology']/@resourceProperties[name='count']"/>
      <resources name="WildcardSelector" extends="//@specification/@domainSpecifications[name='MissionOntology']/@resources[name='Selector']" resourceProperties="//@specification/@domainSpecifications[name='MissionOntology']/@resourceProperties[name='itemType']"/>
