      <resources name="Mission" resourceProperties="//@specification/@domainSpecifications[name='MissionOntology']/@resourceProperties[name='goal'] //@specification/@domainSpecifications[name='MissionOntology']/@resourceProperties[name='responseTimeout'] //@specification/@domainSpecifications[name='MissionOntology']/@resourceProperties[name='missionDeadline']"/>
      <resources name="AgentRequest" resourceProperties="//@specification/@domainSpecifications[name='MissionOntology']/@resourceProperties[name='adaptor_id'] //@specification/@domainSpecifications[name='MissionOntology']/@resourceProperties[name='trs_topic'] //@specification/@domainSpecifications[name='MissionOntology']/@resourceProperties[name='trs_server']"/>
