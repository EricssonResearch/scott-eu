  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/beans_1_1.xsd"
  version="1.1" bean-discovery-mode="all">
  <scan>
    <exclude name="org.jboss.weld.**" />
    <exclude name="jersey.repackaged.**" />
    <exclude name="ch.qos.logback.**" />
    <exclude name="com.sun.**" />
    <exclude name="javassist.**" />
    <exclude name="org.codehaus.**" />
    <exclude name="org.glassfish.hk2.osgiresourcelocator.**" />
    <exclude name="org.glassfish.jersey.**" />
    <exclude name="org.jboss.**" />
    <exclude name="org.eclipse.jetty.**" />
  </scan>
</beans>
